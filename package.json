{
  "name": "com.miloproductionsinc.plugin_tests",
  "displayName": "plugin-tests",
  "version": "1.0.0",
  "description": "An Apache Cordova application that you can use to run plugin tests on.",
  "keywords": [
    "ecosystem:cordova"
  ],
  "author": "Milo Productions Inc",
  "license": "Apache-2.0",
  "scripts": {
    "test": "npm run eslint",
    "eslint": "node node_modules/eslint/bin/eslint www && node node_modules/eslint/bin/eslint src && node node_modules/eslint/bin/eslint tests && node node_modules/eslint/bin/eslint hooks && node node_modules/eslint/bin/eslint servers"
  },
  "dependencies": {
    "cordova-android": "git+https://github.com/apache/cordova-android.git#4cf3dcfaae6dc82ddb1ccf439d209cbcc2f474a0",
    "cordova-plugin-contacts": "^3.0.1",
    "cordova-plugin-test-framework": "^1.1.6"
  },
  "devDependencies": {
    "eslint": "~3.19.0",
    "eslint-config-semistandard": "~11.0.0",
    "eslint-config-standard": "~10.2.1",
    "eslint-plugin-import": "~2.3.0",
    "eslint-plugin-node": "~5.0.0",
    "eslint-plugin-promise": "~3.5.0",
    "eslint-plugin-standard": "~3.0.1",
    "cordova-plugin-whitelist": "1.3.3"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-test-framework": {}
    },
    "platforms": [
      "android"
    ]
  }
}